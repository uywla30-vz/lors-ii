algorithm genesis() -> whole
begin
    datum n : whole = 5;
    datum fact : whole = 1;

    cycle (n > 0) do
        fact = fact * n;
        n = n - 1;
    conclude

    reveal(fact);

    verify (fact == 120) then
        reveal("Factorial correct");
    otherwise
        reveal("Factorial wrong");
    conclude

    result 0;
end
